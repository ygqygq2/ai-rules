{
  "turbo-ai-rules.sources": [
    {
      "id": "ai-rules",
      "name": "ygqygq2's AI Rules",
      "gitUrl": "https://github.com/ygqygq2/ai-rules.git",
      "branch": "main",
      "enabled": true,
      "auth": {
        "type": "none"
      }
    }
  ],
  "turbo-ai-rules.userRules": {
    "directory": ".project-rules",
    "markers": {
      "begin": "<!-- USER-RULES:BEGIN -->",
      "end": "<!-- USER-RULES:END -->"
    }
  },
  "turbo-ai-rules.adapters": {
    "cursor": {
      "enabled": false
    },
    "windsurf": {
      "enabled": false
    },
    "copilot": {
      "enabled": true
    },
    "continue": {
      "enabled": false
    },
    "cline": {
      "enabled": false
    },
    "roo-cline": {
      "enabled": false
    },
    "aider": {
      "enabled": false
    },
    "bolt": {
      "enabled": false
    },
    "qodo-gen": {
      "enabled": false
    }
  },
  "turbo-ai-rules.adapters.custom": [
    {
      "id": "copilot-skills",
      "name": "Copilot Skills",
      "enabled": true,
      "outputPath": ".github/skills",
      "outputType": "directory",
      "fileExtensions": [],
      "organizeBySource": false,
      "generateIndex": true,
      "indexFileName": "{{ruleName}}.md",
      "isRuleType": false
    }
  ]
}
